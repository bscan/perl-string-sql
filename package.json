{
    "name": "perl-string-sql",
    "displayName": "perl-string-sql",
    "description": "highlight perl strings as sql",
    "version": "1.1.0",
    "publisher": "bscan",
    "repository": {
        "url": "https://github.com/bscan/perl-string-sql"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/highlight-sql-string.json",
                "scopeName": "perl-sql.injection",
                "injectTo": [
                    "source.perl"
                ],
                "embeddedLanguages": {
                    "meta.embedded.sql": "sql"
                }
            },
            {
                "path": "./syntaxes/highlight-html-string.json",
                "scopeName": "perl-html.injection",
                "injectTo": [
                    "source.perl"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block.html": "html"
                }
            }
        ]
    }
}
